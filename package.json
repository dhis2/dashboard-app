{
    "name": "dashboard-app",
    "version": "31.0.1",
    "description": "DHIS2 Dashboard app",
    "private": true,
    "license": "BSD-3-Clause",
    "dependencies": {
        "@dhis2/analytics": "^17.0.2",
        "@dhis2/app-runtime": "^2.8.0",
        "@dhis2/app-runtime-adapter-d2": "^1.0.2",
        "@dhis2/d2-i18n": "^1.1.0",
        "@dhis2/d2-ui-core": "^7.1.8",
        "@dhis2/d2-ui-interpretations": "^7.1.8",
        "@dhis2/d2-ui-mentions-wrapper": "^7.2.0",
        "@dhis2/d2-ui-rich-text": "^7.1.8",
        "@dhis2/d2-ui-sharing-dialog": "^7.2.0",
        "@dhis2/d2-ui-translation-dialog": "^7.1.8",
        "@dhis2/data-visualizer-plugin": "^37.0.0",
        "@dhis2/ui": "^6.6.2",
        "classnames": "^2.3.1",
        "d2": "^31.9.3",
        "d2-utilizr": "^0.2.16",
        "i18next": "^20.2.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "react": "^16.14.0",
        "react-dom": "^16.13.1",
        "react-grid-layout": "1.2.2",
        "react-redux": "^7.2.3",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-jsx": "3.3.2"
    },
    "scripts": {
        "start": "d2-app-scripts start",
        "coverage": "npm test -- --coverage",
        "lint": "d2-style js check || d2-style text check",
        "build": "d2-app-scripts build",
        "build:netlify": "yarn build --standalone",
        "test": "d2-app-scripts test",
        "validate-push": "CI=true yarn test",
        "postinstall": "patch-package",
        "cypress:start": "BROWSER=none yarn start",
        "cy:open-live": "cypress_dhis2_api_stub_mode=DISABLED d2-utils-cypress open --appStart 'yarn cypress:start'",
        "cy:run-live": "cypress_dhis2_api_stub_mode=DISABLED d2-utils-cypress run --appStart 'yarn cypress:start'",
        "cy:open-stub": "cypress_dhis2_api_stub_mode=STUB d2-utils-cypress open --appStart 'yarn cypress:start'",
        "cy:run-stub": "cypress_dhis2_api_stub_mode=STUB d2-utils-cypress run --tags '@nonmutating' --appStart 'yarn cypress:start' --record",
        "cy:capture": "cypress_dhis2_api_stub_mode=CAPTURE yarn d2-utils-cypress run --appStart 'yarn cypress:start'"
    },
    "devDependencies": {
        "@dhis2/cli-app-scripts": "^6.1.0",
        "@dhis2/cli-style": "^7.2.2",
        "@dhis2/cli-utils-cypress": "^7.0.1",
        "@dhis2/cypress-commands": "^7.0.1",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.6",
        "d2-manifest": "^1.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "enzyme-to-json": "^3.6.1",
        "eslint-plugin-cypress": "^2.11.2",
        "immutability-helper": "^3.1.1",
        "patch-package": "^6.4.7",
        "postinstall-postinstall": "^2.1.0",
        "redux-mock-store": "^1.5.4"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|sass|scss)$": "identity-obj-proxy"
        }
    }
}

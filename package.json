{
  "name": "dashboards-app",
  "version": "0.1.0",
  "description": "DHIS2 Dashboards app",
  "private": true,
  "devDependencies": {
    "d2-manifest": "^1.0.0",
    "react-scripts": "1.0.7",
    "why-did-you-update": "^0.0.8"
  },
  "dependencies": {
    "d2": "^27.0.0-8",
    "d2-ui": "28.0.0-0",
    "d2-utilizr": "^0.2.13",
    "eslint": "3.19.0",
    "eslint-config-dhis2": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "material-ui": "0.19.0",
    "prop-types": "15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-grid-layout": "^0.14.6",
    "react-redux": "5.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0",
    "rxjs": "^5.4.1"
  },
  "scripts": {
    "prestart": "d2-manifest package.json ./public/manifest.webapp",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "validate": "npm ls --depth 0",
    "lint-all": "eslint src",
    "prebuild": "rm -rf build && npm run validate && npm run lint-all",
    "postbuild": "npm run manifest",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "deploy": "npm run build && mvn clean deploy"
  },
  "manifest.webapp": {
    "name": "Dashboards app",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "DHIS2"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  },
  "proxy": {
      "/dhis-web-core-resource": {
          "target": "http://localhost:8080"
      }
  }
}

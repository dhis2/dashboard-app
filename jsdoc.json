{
    "source": {
        "include": "./src",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|__tests__|__mocks__|__fixtures__)"
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
        "static": true,
        "sort": true
    }
}